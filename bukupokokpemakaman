<script src="Sinyaman%20-%20Simpayjaya_files/jquery.js"></script>
<script>
	$('document').ready(function() {
		setInterval(function() {
			getData()
		}, 10000); // panggil setiap 10 detik
	});


	function getData() {
		$.ajax({
			url: "count.php",
			type: "GET",
			success: function(data) {
				var json = data,
					obj = JSON.parse(json);
				$('#data_pbi').html(obj.data_pbi);
				$('#data_dtks').html(obj.data_dtks);
				$('#data_skm').html(obj.data_skm);
				$('#data_rekomendasi_pa').html(obj.data_rekomendasi_pa);
				$('#data_rekomendasi_kip').html(obj.data_rekomendasi_kip);
				$('#data_pbi_desa').html(obj.data_pbi_desa);
				$('#data_dtks_desa').html(obj.data_dtks_desa);
				$('#data_skm_desa').html(obj.data_skm_desa);
				$('#data_rekomendasi_pa_desa').html(obj.data_rekomendasi_pa_desa);
				$('#data_rekomendasi_kip_desa').html(obj.data_rekomendasi_kip_desa);
				$('#data_jamkesda').html(obj.data_jamkesda);
				$('#data_jamkesda_persalinan').html(obj.data_jamkesda_persalinan);
				$('#data_jamkesda_bayi').html(obj.data_jamkesda_bayi);
				$('#data_jamkesda_desa').html(obj.data_jamkesda_desa);
				$('#data_jamkesda_persalinan_desa').html(obj.data_jamkesda_persalinan_desa);
				$('#data_jamkesda_bayi_desa').html(obj.data_jamkesda_bayi_desa);
			}
		})
	};
</script></body></html>
